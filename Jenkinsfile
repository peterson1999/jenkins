pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Build') {
            steps {
                echo 'Build'
            }
        }
        stage('Finish') {
            steps {
                echo 'Finish'
            }
        }
    }
}
